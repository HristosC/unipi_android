            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#09b680"

            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/text10"
            android:id="@+id/text11"
            android:gravity="center_vertical"
            android:clickable="true"
            android:text="Dr. Costas Xydeas, University of Lancaster, UK" >
        </TextView>
        <TextView
            android:layout_marginTop="20dp"
            android:background="@drawable/background_title"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:textSize="18dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/text11"
            android:id="@+id/text12"
            android:gravity="center"
            android:text="Παροχές Προς Τους Φοιτητές" >
        </TextView>
        <TextView
            android:layout_marginTop="20dp"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#000000"

            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/text12"
            android:id="@+id/text13"
            android:gravity="center_vertical"
            android:text=""
            >

        </TextView>
        <TextView
            android:layout_marginTop="20dp"
            android:background="@drawable/background_title"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:textSize="18dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/text13"
            android:id="@+id/text14"
            android:gravity="center"
            android:text="Υποδομή Έρευνας" >
        </TextView>
        <TextView
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
